-model=\"password\"/><br /><br />\r\n\t\t<input type=\"button\" value=\"登录\" v-on:click=\"getaxios()\"/>\r\n\t\t<input type=\"button\" value=\"注册\" v-on:click=\"getaxios2()\"/>\r\n\t\t<input type=\"button\" value=\"获取数据\" v-on:click=\"getaxios3()\"/>\r\n\t\t<ul>\r\n\t\t\t<li v-for=\"item in data_res\">\r\n\t\t\t\t账号:{{item.userName}}  密码:{{item.password}}\t\t\t\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\t\t</div>\r\n\r\n\t\t<!-- 用户中心 -->\r\n\t\t<div v-show='is_show'> \r\n\t\t\t<h1>用户中心</h1>\r\n\t\t\t<table>\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td>用户名:</td>\r\n\t\t\t\t\t<th>{{username}}</th>\r\n\t\t\t\t\t<td>密码:</td>\r\n\t\t\t\t\t<th>{{password}}</th>\r\n\t\t\t\t</tr>    \r\n\t\t\t</table>   \r\n\t\t\t<input  type=\"text\" placeholder=\"标题\" v-model=\"title\">\r\n\t\t\t<hr/>\r\n\t\t\t<input class=\"srk_input\" type=\"text\" placeholder=\"内容\" v-model=\"content\">\r\n\t\t\t<input type=\"button\" value=\"添加个人数据\"  v-on:click=\"getaxios4()\"> \r\n        </div>\r\n\t</div>\r\n\t\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport dara_arr from './../../api/api.js'\r\nexport default {\r\n\tdata () {\r\n\t\treturn {\r\n\t\t\tusername:'',\r\n\t\t\tpassword:'',\r\n\t\t\tdata_res:[],\r\n\t\t\tis_show:false,\r\n\t\t\ttitle:'',\r\n\t\t\tcontent:''\r\n\t\t}\r\n\t},\r\n\tmethods: {\r\n\t\t//登入\r\n\t\tgetaxios(){\r\n\t\t  \r\n\t\t\tif(this.username&&this.password){\r\n\t\t\t\taxios.post('/api/api/userLogin/',{\r\n\t\t\t\tusername:this.username,\r\n\t\t\t\tpassword:this.password\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t// this.data_res = res;\r\n\t\t\t\tthis.is_show = res.data.status;\r\n\t\t\t\tconsole.log(res);\r\n\t\t\t\t}, res => {\r\n\t\t\t\t\tconsole.info('请求失败');\r\n\t\t\t\t})\r\n\t\t\t}else{\r\n\t\t\t\talert('账号密码不能为空')\r\n\t\t\t}\r\n\t\t},\r\n\t\t//注册\r\n\t\tgetaxios2(){\r\n\t\t\tif(this.username&&this.password){\r\n\t\t\t\taxios.post('/api/api/setUser/',{\r\n\t\t\t\t\tusername:this.username,\r\n\t\t\t\t\tpassword:this.password\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\t// this.data_res = res;\r\n\t\t\t\t\tthis.is_show = res.data.status;\r\n\t\t\t\t\tconsole.log(res);\r\n\t\t\t\t\t}, res => {\r\n\t\t\t\t\t\tconsole.info('请求失败');\r\n\t\t\t\t})\r\n\t\t\t}else{\r\n\t\t\t\talert('账号密码不能为空')\r\n\t\t\t}\r\n\t\t},\r\n\t\t//获取数据\r\n\t\tgetaxios3(){\r\n\t\t\t  console.log(\r\n\t\t\t\tdara_arr\r\n\r\n\t\t\t)\r\n\t\t\taxios.get('/api/api/getArticle/').then(res=>{\r\n\t\t\t\tconsole.log(res);\r\n\t\t\t\tthis.data_res = res.data;\r\n\t\t\t},res=>{\r\n\t\t\t\tconsole.log('请求失败')\r\n\t\t\t})\r\n\t\t},\r\n\t\t//添加个人博客数据\r\n\t\tgetaxios4(){\r\n\t\t\tif(this.username&&this.password){\r\n\t\t\t\taxios.post('/api//api/admin/addArticle/',{\r\n\t\t\t\t\tusername:this.username,\r\n\t\t\t\t\ttitle:this.title,\r\n\t\t\t\t\tcontent:this.content\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\t// this.data_res = res;\r\n\t\t\t\t\tconsole.log(res);\r\n\t\t\t\t\t}, res => {\r\n\t\t\t\t\t\tconsole.info('请求失败');\r\n\t\t\t\t})\r\n\t\t\t}else{\r\n\t\t\t\talert('账号密码不能为空')\r\n\t\t\t}\r\n\t\t},\r\n\r\n\t},\r\n};\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\t#odiv{\r\n\t\twidth: 18.75rem;\r\n\t\theight: 18.75rem;\r\n\t\tmargin: 0 auto;\r\n\t\tbackground: #0C9A9A;\r\n\t\t// padding-top: 1.875rem;\r\n\t}\r\n\ttr{\r\n\t\twidth: 290px;\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-around;\r\n\t}\r\n</style>\r\n"]}]}